{
  "name": "agents-core",
  "version": "1.0.0",
  "description": "Agent-Core Service - Control plane for multi-agent workflow orchestration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@fastify/swagger": "^9.5.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@scalar/fastify-api-reference": "^1.31.4",
    "@sinclair/typebox": "^0.34.33",
    "fastify": "^5.2.1",
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "openai": "^4.20.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0",
    "ioredis": "^5.3.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.6.0",
    "vitest": "^0.34.0",
    "drizzle-kit": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0"
  },
  "keywords": [
    "fastify",
    "typescript",
    "agents",
    "orchestration",
    "multi-agent"
  ],
  "author": "Clancy Team",
  "license": "MIT"
} 